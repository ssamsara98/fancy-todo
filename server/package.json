{
  "name": "fancy-todo-server",
  "version": "1.0.0",
  "description": "Fancy Todo",
  "author": "Sulthon Abdul Malik <sulthon.abdmalik@gmail.com>",
  "license": "MIT",
  "main": "./src/bin/www.ts",
  "scripts": {
    "start": "node ./dist/bin/www",
    "start:dev": "cross-env NODE_ENV=development env-cmd ts-node-dev .",
    "start:debug": "cross-env DEBUG=fancy-todo:* npm run start:dev",
    "prod": "cross-env NODE_ENV=production env-cmd -f .env.production node ./dist/bin/www",
    "prebuild": "rimraf dist",
    "build": "tsc -b",
    "seq": "env-cmd sequelize",
    "seq:ts": "env-cmd ts-node ./node_modules/sequelize-cli/lib/sequelize"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "debug": "^4.3.2",
    "express": "^4.17.1",
    "http-errors": "^1.8.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.7.0",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "^14.x",
    "npm": "^6.x",
    "yarn": "^1.x"
  }
}
